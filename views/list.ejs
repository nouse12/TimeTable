      <tr>
        <td class="one">
        <div class="display">
          <% items[0].items.forEach(function(item){ %>
            <form class="display-form" action="/delete" method="post">
              <input type="checkbox" name="checkbox" value="<%= item._id %>" onChange="this.form.submit()">
              <p class="pp"><%=item.name %></p>
              <p><%=item.time%></p>
              <p><%=item.inputCode %></p>
              <p><%=item.password %></p>
              <input type="hidden" name="listType" value="<%= title %>">
              <input type="hidden" name="day" value="Monday">
            </form>

          <% }) %>

        </div>
        <div class="fill">
          <form class="fill-form" action="/" method="post">
            <input type="text" name="name" placeholder="Enter Subject"><br />
            <input type="text" name="time" placeholder="Enter Time"><br>
            <input type="text" name="inputCode" placeholder="Enter input code"><br>
            <input type="text" name="password" placeholder="password"><br>
            <input type="hidden" name="title" value="<%=title %>">

            <div class="button">
              <button type="submit" name="button" value="Monday">Add New</button>
            </div>
          </form>
        </div>
