    <header class="header">
      <a href="/">Home</a>
      <a href="/Lecture">Lecture</a>
      <a href="/Lab">Lab</a>
      <a href="/Tutorial">Tutorial</a>
    </header>
    <h1><%= title%></h1>
    <table>
      <tr>
        <th class="one">Monday</th>
        <th class="two">Tuesday</th>
        <th class="three">Wednesday</th>
        <th class="four">Thursday</th>
        <th class="five">Friday</th>
        <th class="six">Saturday</th>
        <th class="seven">Sunday</th>
